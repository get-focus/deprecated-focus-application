{
  "name": "focus-application",
  "version": "0.0.9",
  "files": ["lib"],
  "main": "./lib/main.js",
  "devDependencies": {
    "babel-preset-focus": "^0.6.1",
    "chai": "^3.5.0",
    "chai-subset": "^1.3.0",
    "enzyme": "^2.4.1",
    "isomorphic-fetch": "^2.2.1",
    "mocha": "^3.0.2",
    "react-addons-test-utils": "^15.3.1",
    "react-scripts": "0.2.3",
    "redux-thunk": "^2.1.0",
    "material-design-lite": "^1.2.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2"
  },
  "peerDependencies": {
    "material-design-lite": ">= 1.2.0",
    "react": ">= 15.3.1",
    "react-dom": ">= 15.3.1",
    "react-redux": ">= 4.4.5",
    "redux": ">= 3.5.2",
    "redux-thunk": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build-prod": "react-scripts build",
    "prepublish": "npm run build-lib",
    "build-lib": "rimraf lib && node ../babel-focus/babelify.js && npm run copy-sass",
    "eject": "react-scripts eject",
    "test": "mocha src/**/__tests__/**.js",
    "test:watch": "mocha src/**/__tests__/**.js --watch",
    "copy-sass": "cd src && find . -name *.scss | xargs tar cvf - | tar xfp - -C ../lib/"
  },
  "eslintConfig": {
    "extends": "./node_modules/react-scripts/config/eslint.js"
  }
}
